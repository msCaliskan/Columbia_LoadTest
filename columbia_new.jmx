              <stringProp name="parameters"></stringProp>
              <stringProp name="script">import groovy.json.JsonSlurper
import java.util.ArrayList
import java.util.Random

def categoriesJson = new JsonSlurper().parseText(vars.get(&quot;categoriesStr&quot;))

def mainCategories = new ArrayList();
def subCategories = new ArrayList();
def allCategories = new ArrayList();
def randomCategories = new ArrayList();

categoriesJson.props.initialProps.menuItems.each { mainCategory -&gt;
  mainCategories.add(mainCategory.slug)

  mainCategory.children.each { subCategory -&gt;
  	if (subCategory.children.size() == 0) {
  		subCategories.add(subCategory.slug)
  	}

  	subCategory.children.each { category -&gt;
  		allCategories.add(category.slug)
  	}
  }
}
//
//log.info(&quot;GOKHAN &quot; +  mainCategories.size() + &quot; &quot; + mainCategories.toString())
//log.info(&quot;YUSUF &quot; +  subCategories.size() + &quot; &quot; + subCategories.toString())
//log.info(&quot;MUSA &quot; +  allCategories.size() + &quot; &quot; + allCategories.toString())

def selectedMainCategoryIndex = new Random().nextInt(mainCategories.size())
def selectedSubCategoryIndex = new Random().nextInt(subCategories.size())

def noOfRandomCategories = vars.getObject(&quot;NO_OF_RANDOM_CATEGORIES&quot;)

for (i = 0; i &lt; Integer.parseInt(noOfRandomCategories); ++i) {
	def selectedCategoryIndex = new Random().nextInt(allCategories.size())

	vars.put(&quot;randomCategories_&quot;+(i+1), allCategories.get(selectedCategoryIndex).toString())
}

vars.put(&quot;randomCategories_matchNr&quot;, noOfRandomCategories);

//log.info(&quot;SELECTED MC: &quot; + selectedMainCategoryIndex + &quot; SC: &quot; + selectedSubCategoryIndex + &quot; C: &quot; + selectedCategoryIndex)

vars.putObject(&quot;mainCategories&quot;, mainCategories)
vars.putObject(&quot;subCategories&quot;, subCategories)
vars.putObject(&quot;allCategories&quot;, allCategories)

vars.putObject(&quot;selectedMainCategory&quot;, mainCategories.get(selectedMainCategoryIndex))
vars.putObject(&quot;selectedSubCategory&quot;, subCategories.get(selectedSubCategoryIndex))
</stringProp>
